pipeline{

    agent any

    tools{
        maven 'LocalMaven'
        jdk 'LocalJDK'
    }
    stages{

        stage('Build')
        {
            steps{
                echo 'This is build step'
            }
        }
        stage('parallel Stages'){
            parallel{
             
             stage('Test'){
             steps{
                echo 'This is test step'
             }
             }

             stage('Report'){

                 echo 'Generating Report'
             }
            } 
        }
    }
}
